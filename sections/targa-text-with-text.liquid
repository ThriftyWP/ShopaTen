<div class="image-with-text color-{{ section.settings.color_scheme }} background section-{{ section.id }}-padding {{  section.settings.section_class }}">
  <div class="container">
    {% render 'section-text', section: section, class: 'image-with-text', text_align: section.settings.text_align, heading_level: section.settings.heading_level %}

    <div class="image-with-text__content content">
      <div class="image-with-text__grid image-with-text__grid--{{ section.settings.layout }} {% if section.settings.layout == 'text_last' %} image-with-text__grid--reverse{% endif %} image-with-text__grid--{{ section.settings.text_align_inner }}">
        {% if section.settings.layout == 'image_center' %}
          {% if section.blocks.size > 0 %}
            <div class="image-with-text__columns image-with-text__columns--image_center">
              {% for block in section.blocks %}
                {% assign mod = forloop.index | modulo: 2 %}
                {% if mod == 1 %}
                  <div class="image-with-text__column" {{ block.shopify_attributes }}>
                    {% if block.settings.icon != 'none' %}
                      <div
                        class="
                          image-with-text__column-icon
                          image-with-text__column-icon--{{- section.settings.icon_size }}
                          {% if section.settings.box_shadow %}image-with-text__column-icon--shadow {% endif %}
                          image-with-text__column-icon--{{- section.settings.icon_style }}
                          image-with-text__column-icon--{{- block.settings.icon_fill_type }}
                        "
                      >
                        <i
                          class="icon icon-pack icon-{{ block.settings.icon }} {% if section.settings.icon_style == 'colored' and block.settings.icon_fill_type == 'gradient' %} text-gradient {% endif %}"
                        ></i>
                      </div>
                    {% endif %}
                    {% if block.settings.heading != blank %}
                      <div class="image-with-text__column-title h5">
                        {{ block.settings.heading }}
                      </div>
                    {% endif %}
                    {% if block.settings.text %}
                      <div class="image-with-text__column-text">
                        {{ block.settings.text }}
                      </div>
                    {% endif %}
                  </div>
                {% endif %}
              {% endfor %}
            </div>
          {% endif %}
        {% endif %}
        
        {% if section.settings.layout == 'image_center' %}
          {% if section.blocks.size > 0 %}
            <div class="image-with-text__columns image-with-text__columns--image_center">
              {% for block in section.blocks %}
                {% assign mod = forloop.index | modulo: 2 %}
                {% if mod == 0 %}
                  <div class="image-with-text__column" {{ block.shopify_attributes }}>
                    {% if block.settings.icon != 'none' %}
                      <div
                        class="
                          image-with-text__column-icon
                          image-with-text__column-icon--{{- section.settings.icon_size }}
                          {% if section.settings.box_shadow %}image-with-text__column-icon--shadow {% endif %}
                          image-with-text__column-icon--{{- section.settings.icon_style }}
                          image-with-text__column-icon--{{- block.settings.icon_fill_type }}
                        "
                      >
                        <i
                          class="icon icon-pack icon-{{ block.settings.icon }} {% if section.settings.icon_style == 'colored' and block.settings.icon_fill_type == 'gradient' %} text-gradient {% endif %}"
                        ></i>
                      </div>
                    {% endif %}
                    {% if block.settings.heading != blank %}
                      <div class="image-with-text__column-title h5">
                        {{ block.settings.heading }}
                      </div>
                    {% endif %}
                    {% if block.settings.text %}
                      <div class="image-with-text__column-text">
                        {{ block.settings.text }}
                      </div>
                    {% endif %}
                  </div>
                {% endif %}
              {% endfor %}
            </div>
          {% endif %}
        {% endif %}
        {% if section.settings.layout != 'image_center' %}
          {% comment %}targa left column text code {% endcomment %}
          <div class="image-with-text__right text-text-left">
            {%- if section.settings.inner_subheading != blank -%}
              <div class="image-with-text__inner-subheading subtitle">
                {{ section.settings.inner_subheading }}
              </div>
            {%- endif -%}
            {%- if section.settings.inner_heading != blank -%}
              <{{ section.settings.inner_heading_level }} class="image-with-text__inner-heading title--section title--{{ section.settings.inner_heading_line }} h4
                {% if section.settings.inner_word_break and section.settings.inner_heading_line != 'none' %} title--word_break {% endif %}"
                style="--color-line-inner: {{ section.settings.inner_line_color }}">
                {%- if section.settings.inner_heading_image_left != blank -%}
                  <div class="inner-heading-image">
                    <img src="{{ section.settings.inner_heading_image_left | image_url: width: 200 }}" width="" height="" alt="" loading="lazy">
                  </div>
                {%- endif -%}
                {{ section.settings.inner_heading }}
              </{{ section.settings.inner_heading_level }}>
            {%- endif -%}
            {% if section.settings.description != blank %}
              <div class="image-with-text__text body-large">
                {{ section.settings.description }}
              </div>
            {% endif %}
            {%- assign left_blocks = section.blocks | where: 'type', 'left_column' -%}
            {%- if left_blocks.size > 0 -%}
              <div class="image-with-text__columns image-with-text__columns--left">
                {% for block in left_blocks %}
                  <div class="image-with-text__column" {{ block.shopify_attributes }}>
                    <div class="column-top-text">
                      {% if block.settings.heading != blank %}
                        <div class="image-with-text__column-title h5">
                          {{ block.settings.heading }}
                        </div>
                      {% endif %}
                      {% if block.settings.text != blank %}
                        <div class="image-with-text__column-text">
                          {% comment %} theme-check-disable UnknownFilter {% endcomment %}
                          {{ block.settings.text | raw }}
                           {% comment %} theme-check-enable UnknownFilter {% endcomment %}
                        </div>
                      {% endif %}
                    </div>
                    <div class="image-banner__buttons">
                      {% if block.settings.button_label and block.settings.button_link %}
                        <div class="image-with-text__column-buttons tarta-left-button">
                          {% render 'button',
                            link: block.settings.button_link,
                            label: block.settings.button_label,
                            style: block.settings.button_style,
                            class: 'image-with-text__column-button'
                          %}
                        </div>
                      {% endif %}
                      {%- if block.settings.button_label_2 and block.settings.button_link_2 -%}
                        <div class="image-with-text__column-buttons tarta-right-button">
                          {% render 'button',
                            link: block.settings.button_link_2,
                            label: block.settings.button_label_2,
                            style: block.settings.button_style_2,
                            class: 'image-banner'
                          %}
                        </div>
                      {%- endif -%}
                    </div>   
                  </div>
                {% endfor %}
              </div>
            {%- endif -%}
          </div>
          {% comment %}targa right column text code {% endcomment %}
          <div class="image-with-text__right">
            {%- if section.settings.inner_subheading != blank -%}
              <div class="image-with-text__inner-subheading subtitle">
                {{ section.settings.inner_subheading_right }}
              </div>
            {%- endif -%}
            {%- if section.settings.inner_heading != blank -%}
              <{{ section.settings.inner_heading_level }} class="image-with-text__inner-heading title--section title--{{ section.settings.inner_heading_line }} h4
                {% if section.settings.inner_word_break and section.settings.inner_heading_line != 'none' %} title--word_break {% endif %}"
                style="--color-line-inner: {{ section.settings.inner_line_color }}">
                {%- if section.settings.inner_heading_image_right != blank -%}
                  <div class="inner-heading-image">
                    <img src="{{ section.settings.inner_heading_image_right | image_url: width: 200 }}" width="" height="" alt="" loading="lazy">
                  </div>
                {%- endif -%}
                {{ section.settings.inner_heading_right }}
              </{{ section.settings.inner_heading_level }}>
            {%- endif -%}
            {% if section.settings.description != blank %}
              <div class="image-with-text__text body-large">
                {{ section.settings.description_right }}
              </div>
            {% endif %}

            {%- assign right_blocks = section.blocks | where: 'type', 'right_column' -%}
            {%- if right_blocks.size > 0 -%}
              <div class="image-with-text__columns image-with-text__columns--right">
                {% for block in right_blocks %}
                  <div class="image-with-text__column" {{ block.shopify_attributes }}>
                    <div class="column-top-text">
                      {% if block.settings.heading != blank %}
                        <div class="image-with-text__column-title h5">
                          {{ block.settings.heading }}
                        </div>
                      {% endif %}
                      {% if block.settings.text != blank %}
                        <div class="image-with-text__column-text">
                          {% comment %} theme-check-disable UnknownFilter {% endcomment %}
                          {{ block.settings.text | raw }}
                          {% comment %} theme-check-enable UnknownFilter {% endcomment %}
                        </div>
                      {% endif %}
                    </div>
                    <div class="image-banner__buttons">
                      {% if block.settings.button_label and block.settings.button_link %}
                        <div class="image-with-text__column-buttons tarta-left-button">
                          {% render 'button',
                            link: block.settings.button_link,
                            label: block.settings.button_label,
                            style: block.settings.button_style,
                            class: 'image-with-text__column-button'
                          %}
                        </div>
                      {% endif %}
                      {%- if block.settings.button_label_2 and block.settings.button_link_2 -%}
                        <div class="image-with-text__column-buttons tarta-right-button">
                          {% render 'button',
                            link: block.settings.button_link_2,
                            label: block.settings.button_label_2,
                            style: block.settings.button_style_2,
                            class: 'image-banner'
                          %}
                        </div>
                      {%- endif -%}
                    </div> 
                  </div>
                {% endfor %}
              </div>
            {%- endif -%}

            {%- if section.settings.show_button or section.settings.show_button_2 -%}
              <div class="image-with-text__buttons">
                {%- if section.settings.show_button -%}
                  {% render 'button',
                    link: section.settings.button_link,
                    label: section.settings.button_label,
                    style: section.settings.button_style,
                    class: 'image-with-text'
                  %}
                {%- endif -%}
                {%- if section.settings.show_button_2 -%}
                  {% render 'button',
                    link: section.settings.button_link_2,
                    label: section.settings.button_label_2,
                    style: section.settings.button_style_2,
                    class: 'image-with-text'
                  %}
                {%- endif -%}
              </div>
            {% endif %}
          </div>
        {% endif %}
      </div>

      {% if section.settings.layout == 'image_center' %}
        {%- if section.settings.inner_subheading != blank
          or section.settings.inner_heading != blank
          or section.settings.description != blank
        -%}
          <div class="image-with-text__text-center">
            {%- if section.settings.inner_subheading != blank -%}
              <div class="image-with-text__inner-subheading subtitle">
                {{ section.settings.inner_subheading }}
              </div>
            {%- endif -%}
            {%- if section.settings.inner_heading != blank -%}
              <h2 class="image-with-text__inner-heading {{ section.settings.inner_heading_size }}">
                {{ section.settings.inner_heading }}
              </h2>
            {%- endif -%}
            {% if section.settings.description != blank %}
              <div class="image-with-text__text body-large">
                {{ section.settings.description }}
              </div>
            {% endif %}
          </div>
        {% endif %}
        {%- if section.settings.show_button or section.settings.show_button_2 -%}
          <div class="image-with-text__buttons image-with-text__buttons--center">
            {%- if section.settings.show_button -%}
              {% render 'button',
                link: section.settings.button_link,
                label: section.settings.button_label,
                style: section.settings.button_style,
                class: 'image-with-text'
              %}
            {%- endif -%}
            {%- if section.settings.show_button_2 -%}
              {% render 'button',
                link: section.settings.button_link_2,
                label: section.settings.button_label_2,
                style: section.settings.button_style_2,
                class: 'image-with-text'
              %}
            {%- endif -%}
          </div>
        {% endif %}
      {% endif %}
    </div>
  </div>
</div>

{% schema %}
{
  "name": "Text with Text Targa",
  "class": "image-with-text-section spaced-section",
  "enabled_on": {
    "templates": ["*"]
  },
  "settings": [
    {
      "type": "select",
      "id": "layout",
      "options": [
        {
          "value": "image_last",
          "label": "t:sections.image-with-text.settings.layout.options__1.label"
        },
        {
          "value": "text_last",
          "label": "t:sections.image-with-text.settings.layout.options__2.label"
        },
        {
          "value": "image_center",
          "label": "t:sections.image-with-text.settings.layout.options__3.label"
        }
      ],
      "default": "image_last",
      "label": "t:sections.image-with-text.settings.layout.label"
    },
