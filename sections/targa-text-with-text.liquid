{{ 'section-image-with-text.css' | asset_url | stylesheet_tag }}

{%- style -%}
  {% render 'section-padding' %}
  {% render 'section-margin' %}

  .section-{{ section.id }}-padding .image-with-text__media::before {
    background-color: rgba(var(--color-overlay), {{ section.settings.overlay_opacity }});
  }

  {% if section.settings.image != blank %}
    .section-{{ section.id }}-padding .image-with-text__media--adapt,
    .section-{{ section.id }}-padding .image-with-text__media--all-height-adapt {
      padding-bottom: {{ 1 | divided_by: section.settings.image.aspect_ratio | times: 100 | round: 2 }}%;
    }
  {% endif %}

  .image-with-text.color-background-2.targa-text-text {
      background: none;
  }

  .targa-text-text .image-with-text__right {
      background: #F7F8F8;
  }

  .targa-text-text .image-with-text__grid.image-with-text__grid--image_last.image-with-text__grid--top {
    align-items: stretch;}

  .targa-text-text .image-with-text__right.text-text-left {
      flex: 2 1 33%;
      max-width: inherit;
  }

  .targa-text-text .image-with-text__right {
      flex: 1 1 66%;
      max-width: inherit;
      padding: 4rem 0 0 0;
      border-radius: 10px;
  }
  .targa-text-text .text-text-left .image-with-text__columns.image-with-text__columns--left {
    grid-template-columns: inherit;
  } 
  .targa-text-text h3.image-with-text__inner-heading.title--section.title--simple.h4 {
    width: 100%;
    text-align: center;
    color: #000;
    margin-bottom: 0;
  }
  .targa-text-text .image-with-text__column-title.h5 {
    color: #000;
  }
  .targa-text-text .image-with-text__column {
    padding: 4rem;
    justify-content: space-between;
  }
  .targa-text-text .image-with-text__columns--right,.targa-text-text .image-with-text__columns--left {
    margin-top: 2rem;
    border-top: 2px solid #0F1E2D;
  }
  .targa-text-text .image-banner__buttons {
      align-items: center;
      width: 100%;
      justify-content: flex-start;
      display: flex;
  }
  @media screen and (max-width:749px) {
    .targa-text-text .image-banner__buttons {
      justify-content: center;
    }
  }

  .targa-text-text a.image-banner__button.button.button--simple.button--arrow.focus-inset {
      color: #FF4EA4;
  }
  .targa-text-text .image-with-text__column-buttons.tarta-left-button {
    margin-right: 2rem;
  }
  .targa-text-text .image-with-text__columns--right .image-with-text__column:first-of-type {
    border-right: 2px solid rgb(var(--color-border));
  }
{%- endstyle -%}

